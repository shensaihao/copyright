.home_advantage
    padding: 80px 120px
    background-image: url('images/home_advantage_bg.png')
    background-repeat: no-repeat
    height 556px
    background-color: #F5F8FB;
    position relative
    .home_advantage_title
        position absolute
        top: 80px
        left 120px
    .home_advantage_desc
        position: absolute
        left 120px
        bottom: 80px
    .home_advantage_line
        width: 120px;
        height: 3px;
        background: #0665CE;
        margin-top: 15px
    .home_advantage_box
        position absolute
        right: 120px
        top: 80px
    .home_advantage_box_item
        width: 696px;
        padding: 29px 0 29px 0
        background: #FFFFFF;
        box-shadow: 0px 10px 16px 2px rgba(203, 212, 221, 0.16);
        border-radius: 24px;
        .home_advantage_box_item_xueshu_after_img,.home_advantage_box_item_quanwei_after_img,.home_advantage_box_item_ziyuan_after_img
            display: none
    .home_advantage_box_item_xueshu_after,.home_advantage_box_item_ziyuan_after,.home_advantage_box_item_quanwei_after
        display: none
    .home_advantage_box_item_xueshu:hover
        background: #0665CE;
        padding: 30px 40px
        .home_advantage_box_item_xueshu_after
            display: block
        .home_advantage_box_item_xueshu_before_img
            display: none 
        .home_advantage_box_item_xueshu_after_img
            display: block
        .home_advantage_box_item_xueshu_title
            color: #ffffff
    .home_advantage_box_item_ziyuan:hover
        background: #0665CE;
        padding: 30px 40px
        .home_advantage_box_item_ziyuan_after
            display: block
        .home_advantage_box_item_ziyuan_before_img
            display: none 
        .home_advantage_box_item_ziyuan_after_img
            display: block
        .home_advantage_box_item_ziyuan_title
            color: #ffffff
    .home_advantage_box_item_quanwei:hover
        background: #0665CE;
        padding: 30px 40px
        .home_advantage_box_item_quanwei_after
            display: block
        .home_advantage_box_item_quanwei_before_img
            display: none 
        .home_advantage_box_item_quanwei_after_img
            display: block
        .home_advantage_box_item_quanwei_title
            color: #ffffff
    

