            font-weight:400;
            color:rgba(255,255,255,1);
            margin-top: 30px
        .home_header_user_cneter
            font-size:18px;
            font-family:Source Han Sans CN;
            font-weight:400;
            color:rgba(6,101,206,1);
            line-height: 36px
.home_certification
    padding: 80px 0
    width: 100%
    height:606px;
    background:rgba(245,248,251,1);
    overflow hidden
    .home_certification_title_name
        font-size:32px;
        font-family:Source Han Sans CN;
        font-weight:bold;
        color:rgba(40,45,80,1);
        line-height:36px;
    .home_certification_title_desc
        font-size:16px;
        font-family:Source Han Sans CN;
        font-weight:400;
        color:rgba(102,119,145,1);
        line-height:36px;
    .home_certification_line
        width:120px;
        height:3px;
        background:rgba(6,101,206,1);
        margin-top: 13px
    .home_certification_more_more_box
        padding: 0 120px
    .home_certification_more
        font-size:16px;
        font-family:Source Han Sans CN;
        font-weight:400;
        color:rgba(6,101,206,1);
        line-height:36px;
    .home_certification_swiper
        padding: 0 120px
        overflow hidden
        .slide_banquan, .slide_shangbiao, .slide_jishu, .slide_yuanchandi, .slide_chuangtong
            .slide_item_normal
                width: 224px;
                height:315px;
                background:rgba(255,255,255,1);
                box-shadow:0px 10px 16px 2px rgba(203,212,221,0.16);
                border-radius:24px;
                .slide_item_normal_img
                    width: 80px
                    height 80px
                .slide_item_normal_title
                    font-size:20px;
                    font-family:Source Han Sans CN;
                    font-weight:bold;
                    color:rgba(40,45,80,1);
                    line-height:36px;
                .slide_item_normal_lable
                    padding: 0 30px
                    font-size:14px;
                    font-family:Source Han Sans CN;
                    font-weight:400;
                    color:rgba(102,119,145,1);
                    line-height:22px;
                    .slide_item_normal_lable_hover
                        display: none
                .slide_item_normal_line
                    width:224px;
                    height:1px;
                    background:rgba(228,236,249,1);
                .slide_item_normal_btn
                    text-align: center
                    width:120px;
                    height:28px;
                    background:rgba(6,101,206,1);
                    border:1px solid rgba(6,101,206,1);
                    border-radius:6px;
                    font-size:16px;
                    font-weight:400;
                    color:rgba(255,255,255,1);
                    line-height:20px;
            .slide_item_big
                display: none
                width: 468px;
                height: 315px;
                background: #0665CE;
                box-shadow: 0px 10px 16px 2px rgba(203, 212, 221, 0.16);
                border-radius: 24px;
                color: #ffffff
                position relative
                .slide_item_normal_img
                    padding: 15px 8px 15px 15px
                .slide_item_normal_title
                    font-size: 20px;
                    font-family: Source Han Sans CN;
                    font-weight: bold;
                    color: #FFFFFF;
                    line-height: 36px;
                .slide_item_normal_lable
                    font-size: 14px;
                    font-family: Source Han Sans CN;
                    font-weight: 400;
                    color: #FFFFFF;
                    line-height: 22px;
                    opacity: 0.7;
                    padding: 0 20px
                    .slide_item_normal_lable_before
                        display: none
                    .slide_item_normal_lable_hover
                        display: block
                .slide_item_normal_line
                    width: 100%;
                    height: 1px;
                    background: #E4ECF9;
                    opacity: 0.2;
                .slide_item_normal_btn
                    text-align: center
                    width:120px;
                    height:28px;
                    background: #0665CE;
                    border: 1px solid #FFFFFF;
                    border-radius:6px;
                    font-size:16px;
                    font-family:Source Han Sans CN;
                    font-weight:400;
                    color:rgba(255,255,255,1);
                    line-height:20px;
                    position: absolute
                    left 50%
                    margin-left: -60px
        .slide_banquan:hover
            .slide_item_big
                display: block
            .slide_item_normal
                display: none
            + div
                display: none
        .slide_shangbiao:hover
            .slide_item_big
                display: block
            .slide_item_normal
                display: none
            + div
                display: none
        .slide_jishu:hover
            .slide_item_big
                display: block
            .slide_item_normal
                display: none
            + div
                display: none
        .slide_yuanchandi:hover
            .slide_item_big
                display: block
            .slide_item_normal
                display: none
            + div
                display: none
        .slide_chuangtong:hover
            .slide_item_big
                display: block
            .slide_item_normal
                display: none
            
        
